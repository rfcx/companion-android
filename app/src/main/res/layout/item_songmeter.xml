<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/songMeterPrefixesTextView"
        style="@style/AudioMothText.Body.Secondary"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="?attr/selectableItemBackground"
        android:paddingTop="@dimen/margin_padding_normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="PREFIXES-METER" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/songMeterSerialNumberTextView"
        style="@style/AudioMothText.Body.Secondary"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="?attr/selectableItemBackground"
        android:textSize="@dimen/text_xxsmall"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/songMeterPrefixesTextView"
        app:layout_constraintTop_toBottomOf="@id/songMeterPrefixesTextView"
        tools:text="SMM-01269" />

</androidx.constraintlayout.widget.ConstraintLayout>
